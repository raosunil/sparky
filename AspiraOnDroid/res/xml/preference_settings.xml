                android:defaultValue="Name: abcdxyz"
                android:dialogTitle="dialog_asthmaglobals.config.contactName_preference"/>
             <EditTextPreference
            android:key="asthmaglobals.config.contactPhone"
           android:title="title_asthmaglobals.config.contactPhone_preference"
