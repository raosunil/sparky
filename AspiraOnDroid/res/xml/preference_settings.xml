           android:title="title_asthmaglobals.config.maxValues.PEFValue_preference"
                android:summary="summary_asthmaglobals.config.maxValues.PEFValue_preference"
                android:defaultValue="800"
                android:dialogTitle="dialog_asthmaglobals.config.maxValues.PEFValue_preference"/>
